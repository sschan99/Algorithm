#include <iostream>
using namespace std;

int main() {
	int n, x = 1;
	cin >> n;
	for (int i = 1; i <= n; i++) {
		x *= i;
		while (x >= 10) {
			int temp = 0;
			while (x >= 10) {
				temp += x % 10;
				x /= 10;
			}
			x += temp;
		}
	}
	cout << x;
	return 0;
}

